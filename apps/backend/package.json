{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "Express.js backend for MCP proxy server",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --dir __tests__/unit --passWithNoTests",
    "test:integration": "vitest run --dir __tests__/integration --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@local-mcp/config": "workspace:*",
    "@local-mcp/core": "workspace:*",
    "@local-mcp/custom-mcp-loader": "workspace:*",
    "@local-mcp/database": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
