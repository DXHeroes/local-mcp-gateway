{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "Express.js backend for MCP proxy server",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:https": "tsx watch src/index.ts & tsx watch src/dev-https.ts",
    "build": "tsdown",
    "start": "node dist/index.mjs",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --dir __tests__/unit --passWithNoTests",
    "test:integration": "vitest run --dir __tests__/integration --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@local-mcp/config": "workspace:*",
    "@local-mcp/core": "workspace:*",
    "@local-mcp/custom-mcp-loader": "workspace:*",
    "@local-mcp/database": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/http-proxy": "^1.17.17",
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "devcert": "^1.2.2",
    "http-proxy": "^1.18.1",
    "localtunnel": "^2.0.2",
    "supertest": "^7.1.4",
    "tsdown": "^0.16.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
