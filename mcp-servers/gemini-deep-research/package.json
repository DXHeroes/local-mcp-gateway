{
  "name": "@dxheroes/mcp-gemini-deep-research",
  "version": "0.4.4",
  "description": "MCP server providing Google Gemini Deep Research capabilities for AI-powered web research",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "mcpPackage": true,
  "scripts": {
    "build": "tsc --build",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "local-mcp-gateway",
    "gemini",
    "research",
    "ai"
  ],
  "author": "DX Heroes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DXHeroes/local-mcp-gateway.git",
    "directory": "mcp-servers/gemini-deep-research"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@dxheroes/local-mcp-core": "workspace:*"
  },
  "dependencies": {
    "@google/genai": "^1.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
